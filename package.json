{
  "name": "vue-demo",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "webpack serve",
    "build": "cross-env NODE_ENV=production webpack  --progress",
    "test": "jest",
    "lint:style": "stylelint ./src/**/*.less --fix",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx,.vue --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owenvip/vue-demo.git"
  },
  "keywords": [
    "vue",
    "typescript"
  ],
  "author": "owenwong <owen.cq.cn@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/owenvip/vue-demo#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "element-plus": "^1.0.2-beta.44",
    "es6-promise": "^4.2.8",
    "lodash": "^4.17.21",
    "vue": "^3.0.11",
    "vue-router": "^4.0.6",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@otools/compile-tools": "^0.0.6",
    "core-js": "^3.12.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*..{css,less}": [
      "stylelint --fix"
    ],
    "*.{json,md}": [
      "prettier -c --write"
    ]
  },
  "jest": {
    "presets": [
      "@otools/jest-preset-otool"
    ]
  },
  "browserslist": [
    "chrome > 58"
  ],
  "eslintConfig": {
    "extends": [
      "@otools/eslint-config-otool"
    ]
  },
  "prettier": "@otools/prettier-config-otool",
  "stylelint": {
    "extends": [
      "@otools/stylelint-config-otool"
    ]
  }
}
