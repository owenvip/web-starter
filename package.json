{
  "name": "web-starter",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "webpack serve",
    "build": "cross-env NODE_ENV=production webpack  --progress",
    "test": "jest",
    "lint:style": "stylelint ./src/**/*.less --fix",
    "lint": "eslint . --ext=.js,.jsx,.vue --fix",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owenvip/web-starter.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "owenwong <owen.cq.cn@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/owenvip/web-starter#readme",
  "dependencies": {
    "@otools/request": "^0.2.0",
    "blueimp-md5": "^2.19.0",
    "element-plus": "^1.1.0-beta.24",
    "lodash": "^4.17.21",
    "qs": "^6.10.1",
    "vue": "^3.0.11",
    "vue-router": "^4.0.8",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@otools/compile-tools": "^0.0.14",
    "babel-plugin-import": "^1.13.3",
    "core-js": "^3.18.3",
    "cross-env": "^7.0.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,vue}": [
      "eslint --fix"
    ],
    "src/**/*..{css,less}": [
      "stylelint --fix"
    ],
    "*.{json,md}": [
      "prettier -c --write"
    ]
  },
  "jest": {
    "presets": [
      "@otools/jest-preset-otool"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "@otools/eslint-config-otool"
    ]
  },
  "prettier": "@otools/prettier-config-otool",
  "stylelint": {
    "extends": [
      "@otools/stylelint-config-otool"
    ]
  }
}
